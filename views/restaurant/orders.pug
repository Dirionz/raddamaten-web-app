each order in orders
  tr.order-row
    td #{order.email || "Finns ej"}
    td #{(new Date(order.updatedAt - ((new Date()).getTimezoneOffset() * 60000))).toISOString().slice(0,-1).slice(0,16).replace('T', ' ')}
    td #{order.price} kr
    td
      if (order.email)
        span.label.label-success Lyckad
      else 
        span.label.label-danger Misslyckad
    td
      a(href='/restaurant/order/'+order._id) Visa
      .order-for-count
