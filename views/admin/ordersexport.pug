extends ../layout

block content
  .materialize-box
    .material-header
      p Exportera ordrar
    .material-body
      form.form-horizontal(role='form' method='POST') 
        input(type='hidden', name='_csrf', value=_csrf)
        .form-group
          label(class='col-sm-2 control-label', for='startdate') Från
          .col-sm-8
            input.form-control(type='text', name='startdate', value=date, id='date', required)
        .form-group
          label(class='col-sm-2 control-label', for='enddate') Till
          .col-sm-8
            input.form-control(type='text', name='enddate', value=date, id='date', required)
        .form-group
          .col-sm-offset-2.col-sm-8
            button.btn.btn-primary(type='submit')
              i.fa.fa-table
              | Hämta